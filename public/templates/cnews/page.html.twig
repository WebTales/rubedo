		style="position: relative; background: #ffffff;">

		<!-- filet mignon -->
		<div class="row top-bar"
			style="margin-left: -30px; margin-right: -30px; padding-top: 5px; padding-bottom: 5px;"></div>

		{% block pageMainContent %} {% if rows %} {% for row in rows %} {%
		include 'cnews/row.html.twig' with row only %} {% endfor %} {% endif %}
		{% endblock %}

	</div>

	<!-- JS files -->

	<script type='text/javascript' src='/cnews/js/home.js'></script>
	<script type='text/javascript'
		src='/cnews/js/jquery.ui.totop.js?ver=1.1'></script>
	<script type='text/javascript'
