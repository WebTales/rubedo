{% extends "@Root/block.html.twig" %}

{% block mainContent %}
<img src="{{ imageUrl(image) }}" usemap="#{{ prefix }}map">

<map name="{{ prefix }}map">
{% for mapElement in map %}
    <area shape="{{ mapElement.type }}" coords="{{ mapElement.coords }}" href="{{ mapElement.href }}" alt="{{ mapElement.alt }}" title="{{ mapElement.title }}">
{% endfor %}
  
</map>
{% endblock %}